.arguments
   type.like:string
   type.eq:string
   description.like:string
   description.eq:string
auth.ticket.verify:root, admin
add:x:./*/wait.signal/*/args
   get-nodes:x:@.arguments/*
wait.signal:magic.db.mysql.read
   database:anarq
   table:users_extra_types
   args
      limit:long:1
   columns
      count(*) as count
return-nodes:x:@wait.signal/*
