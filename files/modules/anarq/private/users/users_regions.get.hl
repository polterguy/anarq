.arguments
   limit:long
   offset:long
   order:string
   direction:string
   user.like:string
   user.mt:string
   user.lt:string
   user.mteq:string
   user.lteq:string
   user.neq:string
   user.eq:string
   region.like:string
   region.mt:string
   region.lt:string
   region.mteq:string
