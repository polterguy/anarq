.arguments
   post_id:long
   user:string
.description:Creates an item in your likes table in your [generic|anarq] database according to the specified arguments
.type:crud-create
auth.ticket.verify:root, admin
data.connect:[generic|anarq]
   database-type:mysql
   add:x:+/*/values
      get-nodes:x:@.arguments/*
   data.create
      database-type:mysql
      table:likes
      return-id:bool:false
      values
   unwrap:x:+/*
   return
      result:success
